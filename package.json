{
  "name": "signavio-i18n",
  "version": "1.3.1",
  "description": "Minimalist gettext style i18n for JavaScript",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/signavio/i18n.git"
  },
  "bin": {
    "i18n-extract": "./bin/i18n-extract.js",
    "i18n-merge": "./bin/i18n-merge.js"
  },
  "scripts": {
    "clean": "rimraf -rf lib",
    "coverage": "cross-env NODE_ENV=coverage nyc --reporter=lcov --reporter=text yarn test",
    "build": "babel src --out-dir lib",
    "lint": "eslint src specs",
    "test": "mocha-webpack",
    "pretest": "yarn build",
    "prebuild": "yarn run clean",
    "prepublish": "yarn build"
  },
  "author": "Jan-Felix Schwarz",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/signavio/i18n/issues"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "instrument": false,
    "sourceMap": false
  },
  "dependencies": {
    "babel-core": "^6.2.1",
    "gettext-parser": "^1.1.2",
    "glob": "^6.0.1",
    "lodash": "^4.0.0",
    "marked": "^0.3.5",
    "path-exists": "^2.1.0",
    "path-is-absolute": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.5.0",
    "bundle-loader": "^0.5.4",
    "chai": "^3.4.1",
    "chai-enzyme": "^0.6.1",
    "chai-files": "^1.4.0",
    "cross-env": "^4.0.0",
    "enzyme": "^2.8.0",
    "eslint": "^3.12.2",
    "eslint-config-signavio": "^1.4.1",
    "eslint-config-signavio-test": "^1.2.1",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-mocha": "^4.7.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jsdom": "^9.12.0",
    "json-loader": "^0.5.3",
    "mocha": "^2.5.3",
    "mocha-webpack": "^0.7.0",
    "nyc": "^10.3.2",
    "po-loader": "^0.3.1",
    "po2json": "^0.4.2",
    "react": "^15.0.1",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.0.1",
    "rimraf": "^2.5.4",
    "source-map-support": "^0.4.14",
    "webpack": "2.3.2",
    "webpack-dev-server": "2.4.2",
    "webpack-node-externals": "^1.5.4",
    "xunit-file": "^1.0.0"
  },
  "peerDependencies": {
    "react": "^15.0.1"
  }
}
