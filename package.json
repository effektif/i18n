{
  "name": "signavio-i18n",
  "version": "1.3.5",
  "description": "Minimalist gettext style i18n for JavaScript",
  "main": "lib/index.js",
  "module": "lib-es6/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/signavio/i18n.git"
  },
  "bin": {
    "i18n-extract": "./bin/i18n-extract.js",
    "i18n-merge": "./bin/i18n-merge.js"
  },
  "scripts": {
    "prebuild": "yarn run clean",
    "build": "yarn build:es5 && yarn build:es6",
    "build:es5":
      "cross-env NODE_ENV=production BABEL_ENV=es5 babel src --out-dir lib",
    "build:es6":
      "cross-env NODE_ENV=production BABEL_ENV=es6 babel src --out-dir lib-es6",
    "clean": "rimraf lib lib-es6",
    "coverage":
      "cross-env NODE_ENV=coverage nyc --reporter=lcov --reporter=text yarn test",
    "dryrun": "node ./bin/i18n-extract.js test/fixtures/**/*.js",
    "lint": "eslint src specs",
    "test": "cross-env NODE_ENV=es5 mocha-webpack",
    "pretest": "yarn build",
    "prepublishOnly": "yarn build",
    "predryrun": "yarn build",
    "flow": "flow check"
  },
  "author": "Jan-Felix Schwarz",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/signavio/i18n/issues"
  },
  "nyc": {
    "include": ["src/**/*.js"],
    "instrument": false,
    "sourceMap": false
  },
  "dependencies": {
    "babel-core": "^6.2.1",
    "gettext-parser": "^1.1.2",
    "glob": "^6.0.1",
    "lodash": "^4.0.0",
    "marked": "^0.3.5",
    "progress": "^2.0.0"
  },
  "devDependencies": {
    "ajv": "^6.1.1",
    "babel-cli": "^6.2.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-minify-booleans": "^6.8.3",
    "babel-plugin-transform-proto-to-assign": "^6.26.0",
    "babel-plugin-transform-remove-console": "^6.8.5",
    "babel-plugin-transform-remove-debugger": "^6.8.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-2": "^6.5.0",
    "bundle-loader": "^0.5.4",
    "chai": "^3.4.1",
    "chai-enzyme": "^1.0.0-beta.0",
    "chai-files": "^1.4.0",
    "cheerio": "^1.0.0-rc.2",
    "cross-env": "^4.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^3.12.2",
    "eslint-config-signavio": "^1.4.1",
    "eslint-config-signavio-test": "^1.2.1",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-mocha": "^4.7.0",
    "flow-bin": "^0.65.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jsdom": "^9.12.0",
    "json-loader": "^0.5.3",
    "mocha": "^2.5.3",
    "mocha-webpack": "^0.7.0",
    "nyc": "^10.3.2",
    "po-loader": "^0.3.1",
    "po2json": "^0.4.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.5.4",
    "source-map-support": "^0.4.14",
    "webpack": "2.3.2",
    "webpack-dev-server": "2.4.2",
    "webpack-node-externals": "^1.5.4",
    "xunit-file": "^1.0.0"
  },
  "peerDependencies": {
    "react": "^15.0.1 || ^16.0.0"
  }
}
